<!-- 
  Navigation rules from SubsettingPage.xhtml to RequestSummaryPage.xhtml
  ///////////////// start ////////////////
-->
  <navigation-rule>
    <from-view-id>/subsetting/SubsettingPage.xhtml</from-view-id>
    
<!-- download request -->
    <navigation-case>
      <from-action>#{AnalysisPage.dwnldAction}</from-action>
      <from-outcome>success</from-outcome>
      <to-view-id>/subsetting/AnalysisResultsPage.xhtml</to-view-id>
    </navigation-case>

    <navigation-case>
      <from-action>#{AnalysisPage.dwnldAction}</from-action>
      <from-outcome>failure</from-outcome>
      <to-view-id>/subsetting/SubsettingPage.xhtml</to-view-id>
    </navigation-case>
    
    <navigation-case>
      <from-action>#{AnalysisPage.dwnldAction}</from-action>
      <from-outcome>download</from-outcome>
      <to-view-id>/subsetting/SubsettingPage.xhtml</to-view-id>
    </navigation-case>
    
<!-- eda request -->
    <navigation-case>
      <from-action>#{AnalysisPage.edaAction}</from-action>
      <from-outcome>success</from-outcome>
      <to-view-id>/subsetting/AnalysisResultsPage.xhtml</to-view-id>
    </navigation-case>
    
    <navigation-case>
      <from-action>#{AnalysisPage.edaAction}</from-action>
      <from-outcome>failure</from-outcome>
      <to-view-id>/subsetting/SubsettingPage.xhtml</to-view-id>
    </navigation-case>
    
<!-- recode rquest-->
    <navigation-case>
      <from-action>#{AnalysisPage.recodeAction}</from-action>
      <from-outcome>success</from-outcome>
      <to-view-id>/subsetting/SubsettingPage.xhtml</to-view-id>
    </navigation-case>
    
    <navigation-case>
      <from-action>#{AnalysisPage.recodeAction}</from-action>
      <from-outcome>failure</from-outcome>
      <to-view-id>/subsetting/SubsettingPage.xhtml</to-view-id>
    </navigation-case>

<!-- advanced statistics request-->

    <navigation-case>
      <from-action>#{AnalysisPage.advStatAction}</from-action>
      <from-outcome>success</from-outcome>
      <to-view-id>/subsetting/AnalysisResultsPage.xhtml</to-view-id>
    </navigation-case>
    
    <!-- advanced statistics request-->
    <navigation-case>
      <from-action>#{AnalysisPage.advStatAction}</from-action>
      <from-outcome>failure</from-outcome>
      <to-view-id>/subsetting/SubsettingPage.xhtml</to-view-id>
    </navigation-case>

    <navigation-case>
      <from-action>#{AnalysisPage.gotoEditVariableAction}</from-action>
      <from-outcome>editVariable</from-outcome>
      <to-view-id>/study/EditVariablePage.xhtml</to-view-id>
    </navigation-case>   

  </navigation-rule>
